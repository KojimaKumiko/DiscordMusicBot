{
	"name": "discordmusicbot",
	"version": "0.1.0",
	"description": "A discord music bot",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tsc",
		"start": "npx ts-node -r dotenv/config ./src/index.ts dotenv_config_path=config/.env",
		"dev": "npx tsnd -r dotenv/config ./src/index.ts dotenv_config_path=config/.env.dev",
		"commands": "npx tsnd -r dotenv/config ./src/deploy-commands.ts dotenv_config_path=config/.env.dev",
		"commands:prod": "npx tsnd -r dotenv/config ./src/deploy-commands.ts dotenv_config_path=config/.env"
	},
	"author": "Koji",
	"license": "ISC",
	"dependencies": {
		"@discord-player/extractor": "^4.5.1",
		"@discordjs/opus": "^0.9.0",
		"discord-player": "^6.7.1",
		"discord-player-youtubei": "^1.3.5",
		"discord.js": "^14.16.3",
		"dotenv": "^16.4.5",
		"ffmpeg": "^0.0.4",
		"fluent-ffmpeg": "^2.1.3",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"ytdl-core": "^4.11.5"
	},
	"devDependencies": {
		"typescript": "^5.6.3"
	}
}
